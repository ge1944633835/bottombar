<?xml version="1.0" encoding="utf-8"?>
<RelativeLayout
    xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:tools="http://schemas.android.com/tools"
    xmlns:lee="http://schemas.android.com/apk/res-auto"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:orientation="vertical"
    tools:context=".MainActivity">

    <com.me.gcg.bottombar.view.MyBottomBar
        android:id="@+id/bottombar"
        android:layout_alignParentBottom="true"
        lee:bottombar_background="@color/colorPrimary"
        lee:bottombar_tabcount="5"
        lee:bottom_first_select_icon="@mipmap/checkbox_pressed"
        lee:bottombar_first_icon="@mipmap/checkbox_normal"
        lee:bottombar_first_text="消息"
        lee:bottombar_second_text="联系人"
        lee:bottombar_third_text="动态"
        lee:bottombar_forth_text="其他"
        lee:bottombar_fifth_text="其他2"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"/>


</RelativeLayout>
